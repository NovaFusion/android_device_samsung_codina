CHECK_CAPTURE_MODE =       //POLL_FIELD=SEQ_STATE, !=7, DELAY=10, TIMEOUT=200
  $"  >> CAMDRIVER::CHECK_CAPTURE_MODE"

  $s4 01
  [4 14             // loop
    098C A104 $f
    #GET_REG(0990)
    $c0 07       // compare with 3 (return 7 for capture)
      $s4 00     // done
    $cl
      $d000a     // else not done, delay
    $cx
  ]
  $c4 00
  $cl
    $"CAMDRIVER::CHECK_CAPTURE_MODE timeout"
