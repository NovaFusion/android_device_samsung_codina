  $"<< CAMDRIVER::REFRESH_MODE"

; // REFRESH_MODE

////////////////////////////

REFRESH_P=
  098C A103 $f          // Refresh Sequencer
  0990 0005 $f          // = 5
  #POLL_SEQ_CMD_DONE   
; // REFRESH_P

