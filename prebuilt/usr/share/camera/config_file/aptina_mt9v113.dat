CHECK_PREVIEW_MODE =       //POLL_FIELD=SEQ_STATE, !=3, DELAY=10, TIMEOUT=200
  $"  >> CAMDRIVER::CHECK_PREVIEW_MODE"

  $s4 01
  [4 14             // loop
    098C A104 $f
    #GET_REG(0990)
    $c0 03       // compare with 3 (return 3 for preview)
      $s4 00     // done
    $cl
      $d000a     // else not done, delay
    $cx
  ]
  $c4 00
  $cl
    $"CAMDRIVER::CHECK_PREVIEW_MODE timeout"
  $cx

  $"  << CAMDRIVER::CHECK_PREVIEW_MODE"

; // CHECK_PREVIEW_MODE

