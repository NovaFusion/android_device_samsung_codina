  $d000a                            // DELAY=10
  001A 0000 $f                      // RESET_AND_MISC_CONTROL
  $d000a                            // DELAY=10

  0018 4028 $f                      // STANDBY_CONTROL
  #CHECK_SOC_NOT_IN_STANDBY

